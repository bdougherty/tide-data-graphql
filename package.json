{
  "name": "tide-predictions-graphql",
  "version": "2.0.0",
  "description": "A GraphQL service to find the nearest tide prediction station and get predictions for the next few days.",
  "main": "index.js",
  "author": "Brad Dougherty <me@brad.is>",
  "license": "MIT",
  "repository": "bdougherty/tide-predictions-graphql",
  "private": true,
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "micro",
    "dev": "node --inspect=127.0.0.1:9231 node_modules/.bin/micro-dev -p 3002",
    "test": "xo && nyc ava"
  },
  "dependencies": {
    "@turf/distance": "^6.0.1",
    "apollo-server-micro": "^1.3.6",
    "better-title-case": "^1.0.0",
    "datasets-us-states-names-abbr": "^1.0.0",
    "esm": "^3.0.37",
    "geo-tz": "^4.0.1",
    "got": "^8.3.1",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.2",
    "lru-cache": "^4.1.3",
    "merge-graphql-schemas": "^1.5.1",
    "micro": "^9.3.1",
    "micro-compress": "^1.0.0",
    "micro-cors": "0.1.0",
    "micro-route": "^2.5.0",
    "moment-timezone": "^0.5.17"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "eslint-config-bdougherty": "^5.0.1",
    "micro-dev": "^2.1.1",
    "nyc": "^12.0.1",
    "xo": "^0.21.1"
  },
  "now": {
    "alias": "tide-predictions-graphql.now.sh",
    "dotenv": true,
    "env": [
      "ACCESS_CONTROL_ALLOW_ORIGIN",
      "APPLICATION",
      "ENABLE_GRAPHIQL"
    ]
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "xo": {
    "extends": "bdougherty"
  }
}
