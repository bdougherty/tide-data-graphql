{
  "name": "tide-predictions-graphql",
  "version": "2.1.0",
  "description": "A GraphQL service to find the nearest tide prediction station and get predictions for the next few days.",
  "main": "index.js",
  "author": "Brad Dougherty <me@brad.is>",
  "license": "MIT",
  "repository": "bdougherty/tide-predictions-graphql",
  "private": true,
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "micro",
    "dev": "node --inspect=0 node_modules/.bin/micro-dev -p 3102",
    "test": "xo && nyc ava"
  },
  "dependencies": {
    "@turf/bbox": "^6.0.1",
    "@turf/circle": "^6.0.1",
    "@turf/distance": "^6.0.1",
    "apollo-server-micro": "^1.3.6",
    "better-title-case": "^1.0.0",
    "convert-units": "github:ben-ng/convert-units#389fabc",
    "datasets-us-states-names-abbr": "^1.0.0",
    "dotenv": "^6.0.0",
    "esm": "^3.0.44",
    "geo-tz": "^4.0.1",
    "got": "^8.3.1",
    "graphql": "^0.13.2",
    "graphql-iso-date": "^3.5.0",
    "graphql-tools": "^3.0.2",
    "lru-cache": "^4.1.3",
    "merge-graphql-schemas": "^1.5.1",
    "micro": "^9.3.2",
    "micro-route": "^2.5.0",
    "moonmoji": "^1.1.10",
    "suncalc": "^1.8.0",
    "timezonecomplete": "^5.6.0",
    "tzdata": "^1.0.11"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "eslint-config-bdougherty": "^5.1.1",
    "micro-dev": "^3.0.0",
    "nyc": "^12.0.1",
    "xo": "^0.21.1"
  },
  "now": {
    "alias": "tide-predictions-graphql.now.sh",
    "dotenv": true,
    "env": [
      "ACCESS_CONTROL_ALLOW_ORIGIN",
      "APPLICATION",
      "ENABLE_GRAPHIQL",
      "DARK_SKY_API_KEY"
    ]
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "xo": {
    "extends": "bdougherty"
  }
}
