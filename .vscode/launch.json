{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch via NPM",
			"runtimeExecutable": "npm",
			"windows": {
				"runtimeExecutable": "npm.cmd"
			},
			"runtimeArgs": [
				"run",
				"dev"
			],
			"port": 9231,
			"env": {
				"ACCESS_CONTROL_ALLOW_ORIGIN": "*",
				"APPLICATION": "tide dev",
				"ENABLE_GRAPHIQL": "true"
			}
		}
	]
}
